<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.jinonesocial.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_navigation_home_to_mainPage"
            app:destination="@id/navigation_mainPage" />
    </fragment>
    <fragment
        android:id="@+id/navigation_category"
        android:name="com.example.jinonesocial.CategoryFragment"
        android:label="CategoryFragment" />
    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.jinonesocial.CartFragment"
        android:label="CartFragment" />
    <activity
        android:id="@+id/navigation_mainPage"
        android:name="com.example.jinonesocial.MainPage"
        android:label="activity_main_page"
        tools:layout="@layout/activity_main_page" />
</navigation>